    <script>
      $(document).ready(function () {      
        
        $("#funcion").on("change", () =>{       
          const form = {};
          const idFuncion = $("#funcion").val();
          form.idFuncion = idFuncion;
          form.tipo = 3;
          console.log(form);
          $.ajax({
            data: form,
            error: function () {
              alert('error')
            },
          }).done(function (dato){
              $('.selec option').remove();
            for(let i=0; i < dato.length; i++){
              $('.selec').append($("<option>", {
                value: dato[i].id,
                text: dato[i].nombre
            }));

            }
              
            
          })
        })
        
      });

      
