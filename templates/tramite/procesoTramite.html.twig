                            <input type="date" name="fechareunion" id="fechareunion" class="form-control" >        
                          {% else %} 
                            {% set date = dato.fechaReunion()|date('Y-m-d') %}
                            {{ date }}    
                          {% endif %}
                        </div>
                        <div class="form-group col-md-6">
                          <label for="horaFirma" class="">Hora para firmar</label>
                          {% if dato.horaReunion() == null %} 
                            <input type="time" name="horaFirma" id="horaFirma" class="form-control" >
                          {% else %}
                            {% set date = dato.horaReunion()|date('H:i:s') %}
                            {{ date }}
                          {% endif %}
                        </div>
                        <div class="form-group col-md-6">
                          <label for="fechaEjecucion" class="">Fecha de ejecucion</label>
