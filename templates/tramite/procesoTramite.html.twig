  <body id="page-top">
    <!-- Page Wrapper -->
    <div id="wrapper">
      <!-- Sidebar -->
      {{ include('./assets/sidebar.html.twig') }}
      <!-- End of Sidebar -->

      <!-- Content Wrapper -->
      <div id="content-wrapper" class="d-flex flex-column">
        <!-- Main Content -->
        <div id="content">
          <!-- Topbar -->
          {{ include('./assets/navbar.html.twig') }}
          <!-- End of Topbar -->
          
          <!-- Errors -->
          {{ include('components/_errorDatosIncompletos.html.twig') }}
          {{ include('components/_datosIncompletos.html.twig') }}

          <div class='message'>
            {{ include('components/_mensajeRegistro.html.twig') }}
          </div>
          <!-- Begin Page Content -->    
    
          {{ include('tramite/modal/_datosProceso.html.twig') }}
     
          <form class="proceso" enctype="multipart/form-data">      
            <div class=" my-2 container" style="max-width: 950px">
              <div id="wizard">
                <h3>
                  <div class="media">
                    <div class="bd-wizard-step-icon">
                      <img src="https://img.icons8.com/external-kmg-design-outline-color-kmg-design/32/000000/external-office-building-work-from-home-kmg-design-outline-color-kmg-design.png" width="35px"/>
                    </div>
                    <div class="media-body">
                      <div class="bd-wizard-step-title">Minuta y Municipio</div>
                      <div class="bd-wizard-step-subtitle">Paso 1</div>
                    </div>
                  </div>
                </h3>       
                {% for dato in informacion %}  
                {% if estadoProceso != 'FINALIZADO'  %}
                  <section>
                    {# ----------- Content wrapper ----------- #}
                    <div class="content-wrapper">
                      <h4 class="section-heading">Documentos personales </h4>           
                      <div class="form-check">
                        <label class="form-check-label">
                          <input type="hidden"  name="idTramite" id="idTramite" value="{{idTramite}}"  >
                          <input type="checkbox" class="form-check-input" name="cedula" id="cedula"  {% if dato.cedula() == 'true' %}  disabled checked {% endif %} >
                          Copia de cedula 
                        </label>
                      </div>
                      <div class="form-check">
                        <label class="form-check-label">
                          <input type="checkbox" class="form-check-input" name="papeleta" id="papeleta"  {% if dato.papeleta() == 'true' %} disabled checked {% endif %} >
                          Copia de papeleta de votación
                        </label>
                      </div>
                      <div class="form-check">
                        <label class="form-check-label">
                          <input type="checkbox" class="form-check-input" name="bienes" id="bienes"  {% if dato.escrituraBienes() == 'true' %} disabled checked {% endif %} >
                          Escritura de bienes
                        </label>
                      </div>
                      <div class="form-check">       
                        <label class="form-check-label">
                          <input type="checkbox" class="form-check-input" name="enagenado" id="enagenado"  {% if dato.estaEnagenado() == 'true' %} disabled checked {% endif %}  >
                          Certificado de gravámenes
                        </label>
                      </div> 
                      <hr>            
                      <div class="form-group col-md-12">
                        <h4 class="section-heading">Minuta</h4> 
                        {% if dato.minuta() == null %} 
                          <input type="file" name="minuta" id="minuta" class="form-control" placeholder="Cargar minuta">
                        {% else %} 
                          {{ dato.minuta() }}
                        {% endif %}
                      </div>
                      {# ------------ Trámite de donación ------------- #}
                      {% if  'DONA' in dato.getIdClienteTramite().getIdTipoTramiteTransferencia().tramite() %} 
                        <div class="form-group col-md-12">
                          <h4 class="section-heading">Insinuacion de donación</h4> 
                          {% if dato.insinuacionDonacion() == null %} 
                            <input type="file" name="insinuacionDonacion" id="insinuacionDonacion" class="form-control" placeholder="Cargar insinuacion de donacion">
                          {% else %} 
                            {{ dato.insinuacionDonacion() }}
                          {% endif %} 
                        </div>
                      {% endif %}               
                      <div class="form-group col-md-12">
                        <h4 class="section-heading">Valores Municipio </h4>   
                        {% if dato.valoresMunicipio() == null %} 
                          <input type="file" name="vmunicipio" id="vmunicipio" maxlength="5" class="form-control" value="{{ dato.valoresMunicipio() }}" placeholder="Valores a pagar a municipio">
                        {% else %} 
                          {{ dato.valoresMunicipio() }}
                        {% endif %} 
                      </div>
                      <div class="form-check">
                        <label class="form-check-label">
                          <input type="checkbox" class="form-check-input" name="pvalores" id="pvalores"   {% if dato.peticionValores() == 'true' %} disabled checked {% endif %} >
                          Peticion de valores
                        </label>
                      </div>
                      <div class="oculto"> 
                        <hr>
                        <div class="form-group col-md-6">   
                          <a class="btn btn-primary" data-bs-toggle="mi-modal" id="btn-confirm" data-bs-target="mi-modal">Aprobacion de cliente</a>
                        </div>
                        <hr>
                        <div class="form-group col-md-12">
                          <h4 class="section-heading">Comprobante de pago </h4>  
                          {% if dato.pagoValores() == null %} 
                            <input type="file" name="comprobantep" id="comprobantep" class="form-control" placeholder="Cargar comprobante de pago">
                          {% else %} 
                            {{ dato.pagoValores() }}
                          {% endif %} 
                        </div>
                      </div>
                    </div>
                    {# --------------------------------------------- #}
                    <div style="margin: 25px">
                      <button class="btn btn-primary" style="background: #00d69f" type="submit">Guardar</button>
                    </div>
                  </section>
                  <h3>
                    <div class="media">
                      <div class="bd-wizard-step-icon">
                        <img src="https://img.icons8.com/external-kmg-design-flat-kmg-design/32/000000/external-apartment-business-kmg-design-flat-kmg-design.png" width="35px"/>
                      </div>
                      <div class="media-body">
                        <div class="bd-wizard-step-title">Notaria</div>
                        <div class="bd-wizard-step-subtitle">Paso 2</div>
                      </div>
                    </div>
                  </h3>
                  <section>
                  {# ----------- Content wrapper ------------ #}
                    <div class="content-wrapper">
                      <h4 class="section-heading">Detalles de reunion</h4>
                      {# -------------- Row 1 ---------------- #}
