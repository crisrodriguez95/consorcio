  <script >
  

    function sendIdClient(){
    const $formDat = document.querySelector(".proceso");
    const data = new FormData($formDat);
    data.append('cedula', $('#cedula').prop('checked'))
    data.append('papeleta', $('#papeleta').prop('checked'))
    data.append('bienes', $('#bienes').prop('checked'))
    data.append('enagenado', $('#enagenado').prop('checked'))
    data.append('pvalores', $('#pvalores').prop('checked'))
    data.append('pagoNotarial', $('#pagoNotarial').prop('checked'))
    data.append('entregaNotaria', $('#entregaNotaria').prop('checked'))
    data.append('entregaRP', $('#entregaRP').prop('checked'))

   
    for (let [key, value] of data){
      if(key != "documentoFirmado"){
        if(
          value.name == "" || value == "" 
          || data.get("cedula") == "false" 
          || data.get("papeleta") == "false"
          || data.get("bienes") == "false"
          || data.get("enagenado") == "false"
          || data.get("pvalores") == "false"
          || data.get("pagoNotarial") == "false"
          || data.get("entregaNotaria") == "false"
          || data.get("entregaRP") == "false"
          ){
          $('#datosIncompletos').modal('show');
          return;
        }
      }
    }    
    const formData = {};
    const idCliente = '{{ clienteTramite }}';
    formData.tipo = 2;
    formData.idCliente = idCliente;
     $.ajax({
      data: formData,
    }); 

  } 
    function finalizarTramite(){    
    const formData = {};
    const idCliente = '{{ clienteTramite }}';
    formData.tipo = 2;
    formData.idCliente = idCliente;
     $.ajax({
      data: formData,
    }); 

  } 

  var modalConfirm = function(callback){

  $("#btn-confirm").on("click", function(){
    $("#mi-modal").modal('show');
  });

  $("#modal-btn-si").on("click", function(){
    callback(true);
    $("#mi-modal").modal('hide');
  });
  
  $("#modal-btn-no").on("click", function(){
    finalizarTramite() 
    callback(false);
    $("#mi-modal").modal('hide');
  });
};

modalConfirm(function(confirm){
  if(confirm){
    //Acciones si el usuario confirma
    $("#result").html("CONFIRMADO");
  }else{
    //Acciones si el usuario no confirma
    $("#result").html("NO CONFIRMADO");
  }
});
</script>
