         <form class="proceso">   
     
    <div class=" my-2 container" style="max-width: 950px;">
      <div id="wizard">
        <h3>
          <div class="media">
            <div class="bd-wizard-step-icon"><i class="mdi mdi-account-outline"></i></div>
            <div class="media-body">
              <div class="bd-wizard-step-title">Personal Details</div>
              <div class="bd-wizard-step-subtitle">Paso 1</div>
            </div>
          </div>
        </h3>
       
        <section>
            {% for dato in informacion %}  
          <div class="content-wrapper">
            <h4 class="section-heading">Documentos personales </h4>           
            <div class="form-check">
              <label class="form-check-label">
                <input type="checkbox" class="form-check-input" name="cedula" id="cedula"  {% if dato.cedula() == 'true' %} checked {% endif %} >
               Copia de cedula 
              </label>
            </div>
            <div class="form-check">
              <label class="form-check-label">
                <input type="checkbox" class="form-check-input" name="papeleta" id="papeleta"  {% if dato.papeleta() == 'true' %} checked {% endif %} >
               Copia de papeleta de votación
              </label>
            </div>
            <div class="form-check">
              <label class="form-check-label">
                <input type="checkbox" class="form-check-input" name="bienes" id="bienes"  {% if dato.escrituraBienes() == 'true' %} checked {% endif %} >
               Escritura de bienes
              </label>
            </div> 
             <div class="form-check">
              <label class="form-check-label">
                <input type="checkbox" class="form-check-input" name="enagenado" id="enagenado"  {% if dato.estaEnagenado() == 'true' %} checked {% endif %}  >
               Bien enagenado
              </label>
            </div> 
            <hr>            
           
              <div class="form-group col-md-12">
                <label for="minuta" class="">Minuta</label>
                 {% if dato.minuta() == null %} 
                <input type="file" name="minuta" id="minuta" class="form-control" placeholder="Cargar minuta">
                {% else %} 
                {{ dato.minuta() }}
                 {% endif %}
              </div> 
              
              {% if  'DONA' in dato.getIdClienteTramite().getIdTipoTramiteTransferencia().tramite() %} 
              <div class="form-group col-md-12">
                <label for="insinuacionDonacion" class="">Insinuacion de donación</label>
                      {% if dato.insinuacionDonacion() == null %} 
                <input type="file" name="insinuacionDonacion" id="insinuacionDonacion" class="form-control" placeholder="Cargar insinuacion de donacion">
                 {% else %} 
                  {{ dato.insinuacionDonacion() }}
                    {% endif %} 
              </div>
              {% endif %}                                

              
              <div class="form-group col-md-12">
                <label for="vmunicipio" class="">Valores a pagar</label>
                 {% if dato.valoresMunicipio() == null %} 
                <input type="text" name="vmunicipio" id="vmunicipio" class="form-control" value="{{ dato.valoresMunicipio() }}" placeholder="Valores a pagar a municipio">
                 {% else %} 
                  {{ dato.valoresMunicipio() }}
                   {% endif %} 
              </div>           
              <div class="form-check">
              <label class="form-check-label">
                <input type="checkbox" class="form-check-input" name="pvalores" id="pvalores"   {% if dato.peticionValores() == 'true' %} checked {% endif %} >
               Peticion de valores
              </label>
              </div>
              <div class="oculto"> 
              <hr>
              <div class="form-group col-md-6">   
               <a class="btn btn-primary" data-bs-toggle="mi-modal" id="btn-confirm" data-bs-target="mi-modal">Aprobacion de cliente</a>
              </div>
              <hr>
              <div class="form-group col-md-12">
                <label for="comprobantep" class="">Subir comprobante de pago</label>
                {% if dato.pagoValores() == null %} 
                <input type="file" name="comprobantep" id="comprobantep" class="form-control" placeholder="Cargar comprobante de pago">
                {% else %} 
                  {{ dato.pagoValores() }}
                {% endif %} 
              </div>
              </div>

          </div>
        </section>
        <h3>
          <div class="media">
            <div class="bd-wizard-step-icon"><i class="mdi mdi-bank"></i></div>
            <div class="media-body">
              <div class="bd-wizard-step-title">Employ Details</div>
              <div class="bd-wizard-step-subtitle">Paso 2</div>
            </div>
          </div>
        </h3>
        <section>
          <div class="content-wrapper">
            <h4 class="section-heading">Detalles de reunion</h4>
            <div class="row">
              <div class="form-group col-md-6">
                <label for="fechareunion" class="">Fecha para firmar</label>
                <input type="date" name="fechareunion" id="fechareunion" class="form-control" >        

              </div>
              <div class="form-group col-md-6">
                <label for="horaFirma" class="">Hora para firmar</label>
                  <input type="time" name="horaFirma" id="horaFirma" class="form-control" >
              </div>
              <div class="form-group col-md-6">
                <label for="fechaEjecucion" class="">Fecha de ejecucion</label>
                <input type="date" name="fechaEjecucion" id="fechaEjecucion" class="form-control" >          

              </div>
              <div class="form-group col-md-6">
                <label for="valorTasaNotarial" class="">Valor tasa notarial</label>
                  <input type="text" name="valorTasaNotarial" id="valorTasaNotarial" class="form-control" >
              </div>
              <div class="form-check">
              <label class="form-check-label">
                <input type="checkbox" class="form-check-input" name="" id="" value="checkedValue" >
               Pago tasa notarial
              </label>
              </div>
            </div>
            <hr>
            <div class="row">
              <div class="form-check col-md-12">
                <label class="form-check-label">
                <input type="checkbox" class="form-check-input" name="" id="" value="checkedValue" >
               ¿Es mutualista?
              </label>
              </div>
              
              <div class="form-check col-md-12">
                <label class="form-check-label">
                <input type="checkbox" class="form-check-input" name="" id="" value="checkedValue" >
               Documentacion entregada a mutualista
              </label>
              </div>              
              <div class="form-group col-md-12">
                <label for="minuta" class="">Subir documentacion firmada</label>
                <input type="file" name="minura" id="minuta" class="form-control" placeholder="Cargar minuta">
              </div> 
              <div class="form-check col-md-12">
                <label class="form-check-label">
                <input type="checkbox" class="form-check-input" name="" id="" value="checkedValue" >
               Documentacion entregada en notaria
              </label>
              </div>            
              <div class="form-group col-md-12">
                <label for="minuta" class="">Subir escritura</label>
                <input type="file" name="minura" id="minuta" class="form-control" placeholder="Cargar minuta">
              </div> 
              <div class="form-check col-md-12">
                <label class="form-check-label">
                <input type="checkbox" class="form-check-input" name="" id="" value="checkedValue" >
               Documentacion entregada a registro de la propiedad
              </label>
              </div>  
              <div class="form-check">
              <label class="form-check-label">
                <input type="checkbox" class="form-check-input" name="" id="" value="checkedValue" >
                Aprobacion de pago de valores de cliente
              </label>
              </div>          

            </div>
          </div>
        </section>
        <h3>
          <div class="media">
            <div class="bd-wizard-step-icon"><i class="mdi mdi-account-check-outline"></i></div>
            <div class="media-body">
              <div class="bd-wizard-step-title">Review </div>
              <div class="bd-wizard-step-subtitle">Paso 3</div>
            </div>
          </div>
        </h3>
        <section>
          <div class="content-wrapper">
          <h4 class="section-heading mb-5">Detalles</h4>
          <h6 class="font-weight-bold">Personal Details</h6>
           <div class="form-check col-md-12">
                <label class="form-check-label">
                <input type="checkbox" class="form-check-input" name="" id="" value="checkedValue" >
               Titulo de pago entregado
              </label>
            </div> 
            <div class="form-check col-md-12">
                <label class="form-check-label">
                <input type="checkbox" class="form-check-input" name="" id="" value="checkedValue" >
             Pago de valores
              </label>
            </div> 
            <div class="form-check col-md-12">
                <label class="form-check-label">
                <input type="checkbox" class="form-check-input" name="" id="" value="checkedValue" >
              Escritura valida
              </label>
            </div> 
            
            <div class="form-group col-md-12">
                <label for="minuta" class="">Subir acta de inscripcion</label>
                <input type="file" name="minura" id="minuta" class="form-control" placeholder="Cargar minuta">
              </div> 
              <div class="form-group col-md-12">
                <label for="minuta" class="">Subir informe de gastos</label>
                <input type="file" name="minura" id="minuta" class="form-control" placeholder="Cargar minuta">
              </div> 
            
              <div class="form-check col-md-12">
                <label class="form-check-label">
                <input type="checkbox" class="form-check-input" name="" id="" value="checkedValue" >
               Pago de gastos
              </label>
              </div>
          </div>
        </section>
        
        <h3>
          <div class="media">
            <div class="bd-wizard-step-icon"><i class="mdi mdi-emoticon-outline"></i></div>
            <div class="media-body">
              <div class="bd-wizard-step-title">Submit</div>
              <div class="bd-wizard-step-subtitle">Step 4</div>
            </div>
          </div>
        </h3>
        <section>
          <div class="content-wrapper">
            <h4 class="section-heading mb-5">Resumen de Tramite</h4>
            <div class="form-check">
              <label class="form-check-label">
                <input type="checkbox" class="form-check-input" name="" id="" value="checkedValue" checked>
                I hereby declare that I had read all the <a href="#!">terms and conditions</a>  and all the details provided my me in this form are true.
              </label>
            </div>
          </div>  
        </section>
          <button class="btn btn-primary" type="submit">Guardar</button>
      </div>
      
      </form> 

    </div>
