                          {% else %} 
                            {% set date = dato.fechaEjecucion()|date('Y-m-d') %}
                            {{ date }}
                          {% endif %}
                        </div>
                        <div class="form-group col-md-12">
                          <h4 class="section-heading">Valor tasa notarial</h4>  
                          {% if dato.pagoTasaNotarial() == null %} 
                            <input type="file" name="valorTasaNotarial" id="valorTasaNotarial" class="form-control" value= "" >
                          {% else %} 
                            {{ dato.pagoTasaNotarial()  }}
                          {% endif %}
                        </div>
                        <div class="form-check">
                          <label class="form-check-label">
                            <input type="checkbox" class="form-check-input" name="pagoNotarial" id="pagoNotarial" {% if dato.pagoCompleto() == 'true' %} disabled checked {% endif %} >
                            Pago tasa notarial
                          </label>
                        </div>
                      </div>
                      <hr>
                      {# -------------- Row 2 ---------------- #}
                      <div class="row">
                        <div class="form-check col-md-12">
                          <h4 class="section-heading">Mutualista</h4> 
                          <label class="form-check-label mut">
                            <input type="checkbox" class="form-check-input" name="esMutualista" id="esMutualista"  {% if dato.esMutualista() == 'true' %} disabled checked {% endif %} >
                            ¿Es mutualista?
                          </label>
                        </div>

                        {# <div class="form-check col-md-12">
                          <label class="form-check-label">
                            <input type="checkbox" class="form-check-input" name="entregadoMutualista" id="entregadoMutualista" {% if dato.entregadoMutualista() == 'true' %} disabled checked {% endif %}>
                            Documentacion entregada a consorcio
                          </label>
                        </div>  #}
                          
                        <div class="form-group col-md-12 documFirmado" style="padding-top: 14px">
                          <h4 class="section-heading">Subir documentación firmada</h4> 
                          {% if dato.documentoFirmado() == null %} 
                            <input type="file" name="documentoFirmado" id="documentoFirmado" class="form-control" placeholder="Cargar minuta">
                          {% else %} 
                            {{ dato.documentoFirmado() }}
                          {% endif %}
                        </div>
          
                        <div class="form-check col-md-12">
                          <h4 class="section-heading">Notaria</h4>
                          <input type="checkbox" class="form-check-input" name="entregaNotaria" id="entregaNotaria" {% if dato.entregadaNotaria() == 'true' %} disabled checked {% endif %} >
                          Documentacion entregada en notaria
                        </div>

                        <div class="form-group col-md-12" style="padding-top: 14px">
                          <h4 class="section-heading">Subir escritura</h4> 
                          {% if dato.subirEscritura() == null %} 
                            <input type="file" name="escritura" id="escritura" class="form-control" placeholder="Cargar minuta">
                          {% else %}
                            {{ dato.subirEscritura() }}
                          {% endif %}
                        </div> 

                        <div class="form-check col-md-12">
                          <h4 class="section-heading">Registro de la Propiedad</h4>
                          <input type="checkbox" class="form-check-input" name="entregaRP" id="entregaRP" {% if dato.entregadaRegistroPropiedad() == 'true' %} disabled checked {% endif %} >
                          Documentacion entregada a registro de la propiedad
                        </div>

                        <div class="form-check">
                          <label class="form-check-label">
                            <input type="checkbox" class="form-check-input" name="clienteAprueba" id="clienteAprueba" {% if dato.clienteAprueba() == 'true' %} disabled checked {% endif %}  >
                            Aprobacion de pago de valores de cliente
                          </label>
                        </div>         
                      </div>
                    </div>
                    {# --------------------------------- #}
                    <div style="margin: 25px">
                      <button class="btn btn-primary" style="background: #00d69f" type="submit">Guardar</button>
                    </div>
                  </section>
                
                  <h3>
                    <div class="media">
                      <div class="bd-wizard-step-icon">
                        <img src="https://img.icons8.com/emoji/48/000000/scroll-emoji.png" width="35px"/>
                      </div>
                      <div class="media-body">
                        <div class="bd-wizard-step-title">Escritura e Informe de gastos</div>
                        <div class="bd-wizard-step-subtitle">Paso 3</div>
                      </div>
                    </div>
                  </h3>

                  <section>
                  {# --------------Content wrapper -------------- #}
                    <div class="content-wrapper">
                      <h4 class="section-heading mb-5">Detalles</h4>
                      <div class="form-check col-md-12">
                        <label class="form-check-label">
                          <input type="checkbox" class="form-check-input" name="tituloPago" id="tituloPago" {% if dato.tituloPagoEntregado() == 'true' %} disabled checked {% endif %}  >
                          Titulo de pago entregado
                        </label>
                      </div> 

                      <div class="form-check col-md-12">
                        <label class="form-check-label">
                          <input type="checkbox" class="form-check-input" name="pagoTitulo" id="pagoTitulo" {% if dato.tituloPago() == 'true' %} disabled checked {% endif %} >
                          Pago de valores
                        </label>
                      </div> 
                      <hr>

                      <div class="form-check col-md-12">
                        <h4 class="section-heading">Escritura</h4>
                        <label class="form-check-label">
                          <input type="checkbox" class="form-check-input" name="escrituraVali" id="escrituraVali" {% if dato.escrituraValida() == 'true' %} disabled checked {% endif %} >
                          Escritura valida
                        </label>
                      </div>

                      <div class="form-group col-md-12" style="padding-top: 14px">
                        <h4 class="section-heading">Subir acta de acta de inscripción</h4> 
                        {% if dato.actaInscripcion() == null %} 
                          <input type="file" name="acta" id="acta" class="form-control" placeholder="Cargar minuta">
                        {% else %} 
                          {{ dato.actaInscripcion() }}
                        {% endif %}
                      </div>

                      <div class="form-group col-md-12">
                        <h4 class="section-heading">Informe de gastos</h4>
                        {% if dato.informeGastos() == null %} 
                          <input type="file" name="gastos" id="gastos" class="form-control" placeholder="Cargar minuta">
                        {% else %} 
                          {{ dato.informeGastos() }}
                        {% endif %}
                      </div> 
        
                      <div class="form-check col-md-12">
                        <label class="form-check-label">
                          <input type="checkbox" class="form-check-input" name="pagoGastos" id="pagoGastos" {% if dato.pagoGastos() == 'true' %} disabled checked {% endif %} >
                          Pago de gastos
                        </label>
                      </div>
                    </div>
                    {# -------------------------------------- #}
                    <div style="margin: 25px">
                      <button class="btn btn-primary" style="background: #00d69f" type="submit">Guardar</button>
                    </div>
                  </section>

                  <h3>
                    <div class="media">
                      <div class="bd-wizard-step-icon">
                        <img src="https://img.icons8.com/external-flat-geotatah/64/000000/external-clearage-workaholic-flat-flat-geotatah.png" width="35px"/>
                      </div>
                      <div class="media-body">
                        <div class="bd-wizard-step-title">Finalización</div>
                        <div class="bd-wizard-step-subtitle">Paso 4</div>
                      </div>
                    </div>
                  </h3>

                  <section>                   
                    <div class="content-wrapper">
                      <h4 class="section-heading mb-5">Resumen de Tramite</h4>
                        <div class="form-check">
                          <label class="form-check-label">
                            <input type="checkbox" class="form-check-input" name="" id="" value="checkedValue" checked>
                            Antes de finalizar el trámite verifique que todos los datos se han guardado correctamente. Si todo esta listo puede dar click en finalizar.</label>
                          </label>
                        </div>
                      </div>
                      <div style="margin: 25px">
                        <button type="button" class="btn btn-primary fin" onClick="sendIdClient()"  style="background: #00d69f">Finalizar</button>
                      </div>
                    {% else %}
                      {# ---------------------Else Trámite Finalizado---------------------- #}
                  <section>
                    {# ----------- Content wrapper ----------- #}
                    <div class="content-wrapper">
                      <h4 class="section-heading">Documentos personales </h4>           
                      <div class="form-check">
                        <label class="form-check-label">
                          <input type="hidden"  name="idTramite" id="idTramite" value="{{idTramite}}"  >
                          <input type="checkbox" class="form-check-input" name="cedula" id="cedula" disabled {% if dato.cedula() == 'true' %}  disabled checked {% endif %} >
                          Copia de cedula 
                        </label>
                      </div>
                      <div class="form-check">
                        <label class="form-check-label">
                          <input type="checkbox" class="form-check-input" name="papeleta" id="papeleta" disabled {% if dato.papeleta() == 'true' %} disabled checked {% endif %} >
                          Copia de papeleta de votación
                        </label>
                      </div>
                      <div class="form-check">
                        <label class="form-check-label">
                          <input type="checkbox" class="form-check-input" name="bienes" id="bienes" disabled {% if dato.escrituraBienes() == 'true' %} disabled checked {% endif %} >
                          Escritura de bienes
                        </label>
                      </div>
                      <div class="form-check">       
                        <label class="form-check-label">
                          <input type="checkbox" class="form-check-input" name="enagenado" id="enagenado" disabled {% if dato.estaEnagenado() == 'true' %} disabled checked {% endif %}  >
                          Certificado de gravámenes
                        </label>
                      </div> 
                      <hr>            
                      <div class="form-group col-md-12">
                        <h4 class="section-heading">Minuta</h4> 
                        {% if dato.minuta() == null %} 
                          <input type="file" name="minuta" id="minuta" class="form-control" placeholder="Cargar minuta" disabled>
                        {% else %} 
                          {{ dato.minuta() }}
                          <a class="file" style="padding: 4px 15px; background: #4A2950; color: #fff; float: right; border-radius: 5px" target="_blank" href="../archivos/{{ idTramite }}/{{ dato.minuta() }}" > 
                            <i class="fas fa-download"></i>
                            Descargar 
                          </a>                                                                             
                        {% endif %}
                      </div>
                      {# ------------ Trámite de donación ------------- #}
                      {% if  'DONA' in dato.getIdClienteTramite().getIdTipoTramiteTransferencia().tramite() %} 
                        <div class="form-group col-md-12">
                          <label for="insinuacionDonacion" class="">Insinuacion de donación</label>
                          {% if dato.insinuacionDonacion() == null %} 
                            <input type="file" name="insinuacionDonacion" id="insinuacionDonacion" class="form-control" placeholder="Cargar insinuacion de donacion" disabled>
                          {% else %} 
                            {{ dato.insinuacionDonacion() }}
                            <a class="file" style="padding: 4px 15px; background: #4A2950; color: #fff; float: right; border-radius: 5px;" target="_blank" href="../archivos/{{ idTramite }}/{{ dato.insinuacionDonacion() }}" > 
                              <i class="fas fa-download"></i>
                              Descargar 
                            </a>                                                                             
                          {% endif %} 
                        </div>
                      {% endif %}               
                      <div class="form-group col-md-12">
                        <h4 class="section-heading">Valores Municipio </h4>   
                        {% if dato.valoresMunicipio() == null %} 
                          <input type="file" name="vmunicipio" id="vmunicipio" maxlength="5" class="form-control" value="{{ dato.valoresMunicipio() }}" placeholder="Valores a pagar a municipio" disabled>
                        {% else %} 
                          {{ dato.valoresMunicipio() }}
                          <a class="file" style="padding: 5px 15px; background: #4A2950; color: #fff; float: right; border-radius: 5px" target="_blank" href="../archivos/{{ idTramite }}/{{ dato.valoresMunicipio() }}" >
                           <i class="fas fa-download"></i>
                           Descargar 
                          </a>                                                                             
                        {% endif %} 
                      </div>
                      <div class="form-check">
                        <label class="form-check-label">
                          <input type="checkbox" class="form-check-input" name="pvalores" id="pvalores" disabled  {% if dato.peticionValores() == 'true' %} disabled checked {% endif %} >
                          Peticion de valores
                        </label>
                      </div>
                      <div class="oculto"> 
                      
                        <hr>
                        <div class="form-group col-md-12">
                          <h4 class="section-heading">Comprobante de pago </h4>  
                          {% if dato.pagoValores() == null %} 
                            <input type="file" name="comprobantep" id="comprobantep" class="form-control" placeholder="Cargar comprobante de pago" disabled>
                          {% else %} 
                            {{ dato.pagoValores() }}
                            <a class="file" style="padding: 5px 15px; background: #4A2950; color: #fff; float: right; border-radius: 5px" target="_blank" href="../archivos/{{ idTramite }}/{{ dato.pagoValores() }}" > 
                              <i class="fas fa-download"></i>
                              Descargar 
                            </a>                                                   
                          {% endif %} 
                        </div>
                      </div>
                    </div>
                    {# --------------------------------------------- #}
                  </section>
                  <h3>
                    <div class="media">
                      <div class="bd-wizard-step-icon">
                        <img src="https://img.icons8.com/external-kmg-design-flat-kmg-design/32/000000/external-apartment-business-kmg-design-flat-kmg-design.png" width="35px"/>
                      </div>
                      <div class="media-body">
                        <div class="bd-wizard-step-title">Notaria</div>
                        <div class="bd-wizard-step-subtitle">Paso 2</div>
                      </div>
                    </div>
