                          {% else %}
                            {% set date = dato.horaReunion()|date('H:i:s') %}
                            {{ date }}
                          {% endif %}
                        </div>
                        <div class="form-group col-md-6">
                          <label for="fechaEjecucion" class="">Fecha de ejecucion</label>
                          {% if dato.fechaEjecucion() == null %}  
                            <input type="date" name="fechaEjecucion" id="fechaEjecucion" class="form-control" disabled>          
